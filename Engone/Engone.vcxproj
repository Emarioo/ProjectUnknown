  <PropertyGroup Label="UserMacros" />
  <!-- Output -->
  <PropertyGroup>
    <LinkIncremental>true</LinkIncremental>
    <OutDir>$(SolutionDir)bin\$(ProjectName)\$(Configuration)$(Platform)</OutDir>
    <IntDir>$(SolutionDir)bin\intermediate\$(ProjectName)\$(Configuration)$(Platform)</IntDir>
  </PropertyGroup>
  <!-- base, ignoring x64 for now-->
  <ItemDefinitionGroup>
    <ClCompile>
      <WarningLevel>Level3</WarningLevel>
      <SDLCheck>true</SDLCheck>
      <PreprocessorDefinitions>WIN32;_CRT_SECURE_NO_WARNINGS;</PreprocessorDefinitions>
      <ConformanceMode>true</ConformanceMode>
      <AdditionalIncludeDirectories>include;$(SolutionDir)\libs\GLFW\include;$(SolutionDir)\libs\GLEW\include;$(SolutionDir)\libs\ASIO\include</AdditionalIncludeDirectories>
      <LanguageStandard>stdcpp17</LanguageStandard>
      <PrecompiledHeader>Use</PrecompiledHeader>
      <PrecompiledHeaderFile>Engone/pch.h</PrecompiledHeaderFile>
      <DisableSpecificWarnings>%(DisableSpecificWarnings)</DisableSpecificWarnings>
      <ForcedIncludeFiles>Engone/pch.h;Engone/ModuleDefs.h</ForcedIncludeFiles>
      <RuntimeLibrary>MultiThreadedDebug</RuntimeLibrary>
    </ClCompile>
    <Link>
      <GenerateDebugInformation>true</GenerateDebugInformation>
    </Link>
    <Lib>
      <AdditionalLibraryDirectories>$(SolutionDir)\libs\GLFW\lib;$(SolutionDir)\libs\GLEW\lib</AdditionalLibraryDirectories>
      <AdditionalDependencies>gdi32.lib;user32.lib;shell32.lib;opengl32.lib;glew32s.lib;glfw3.lib</AdditionalDependencies>
    </Lib>
    <ProjectReference>
      <LinkLibraryDependencies>false</LinkLibraryDependencies>
    </ProjectReference>
  </ItemDefinitionGroup>
  <!-- debug -->
  <ItemDefinitionGroup Condition="$(Configuration.StartsWith('Deb'))">
    <ClCompile>
      <PreprocessorDefinitions>%(PreprocessorDefinitions);_DEBUG;_CONSOLE;</PreprocessorDefinitions>
      <RuntimeLibrary>MultiThreadedDebug</RuntimeLibrary>
    </ClCompile>
    <Link>
      <SubSystem>Console</SubSystem>
    </Link>
  </ItemDefinitionGroup>
  <!-- release -->
  <ItemDefinitionGroup Condition="$(Configuration.StartsWith('Rel'))">
    <ClCompile>
      <FunctionLevelLinking>true</FunctionLevelLinking>
      <IntrinsicFunctions>true</IntrinsicFunctions>
      <RuntimeLibrary>MultiThreaded</RuntimeLibrary>
    </ClCompile>
    <Link>
      <!-- <SubSystem>Console</SubSystem> -->
      <SubSystem>Windows</SubSystem>
      <EnableCOMDATFolding>true</EnableCOMDATFolding>
      <OptimizeReferences>true</OptimizeReferences>
    </Link>
  </ItemDefinitionGroup>
  <!-- specifics -->
  <ItemDefinitionGroup Condition="$(Configuration.Contains('Phys'))">
    <ClCompile>
      <PreprocessorDefinitions>%(PreprocessorDefinitions);ENGONE_PHYSICS</PreprocessorDefinitions>
      <AdditionalIncludeDirectories>%(AdditionalIncludeDirectories);$(SolutionDir)libs\reactphysics3d\include</AdditionalIncludeDirectories>
    </ClCompile>
    <Lib>
      <AdditionalLibraryDirectories Condition="$(Configuration.StartsWith('Deb'))">%(AdditionalLibraryDirectories);$(SolutionDir)..\ReactPhysics\build\Debug</AdditionalLibraryDirectories>
      <AdditionalLibraryDirectories Condition="$(Configuration.StartsWith('Rel'))">%(AdditionalLibraryDirectories);$(SolutionDir)..\ReactPhysics\build\Release</AdditionalLibraryDirectories>
      <AdditionalDependencies>%(AdditionalDependencies);reactphysics3d.lib</AdditionalDependencies>
    </Lib>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Condition="$(Configuration.Contains('Sound'))">
    <Message Importance="High" Text="Building with sound is not finished." />
    <!-- Sound is pretty much finished in here but OpenAL32 requires you to install it on your computer which I don't want.
          I just want a static lib without any installing which works nicely with the executable. -->
    <ClCompile>
      <PreprocessorDefinitions>%(PreprocessorDefinitions);ENGONE_SOUND</PreprocessorDefinitions>
      <AdditionalIncludeDirectories>%(AdditionalIncludeDirectories);$(SolutionDir)\libs\AL\include</AdditionalIncludeDirectories>
    </ClCompile>
    <Lib>
      <AdditionalLibraryDirectories>%(AdditionalLibraryDirectories);$(SolutionDir)\libs\AL\lib;</AdditionalLibraryDirectories>
      <AdditionalDependencies>%(AdditionalDependencies);OpenAL32.lib</AdditionalDependencies>
    </Lib>
  </ItemDefinitionGroup>
